/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.fl{
  float: left;
}
.fr{
  float: right;
}
.show{
  display: block;
}
.hide{
  display: none;
}
page{
  width: 100%;
  height: 100%;
  background: url('https://wxjingtu.top/image/bg.png') no-repeat;
  background-size: 100% 100%;
  color: #fff;
}
.btnLarge{
  width: 640rpx;
  height: 80rpx;
  line-height: 80rpx;
  color: #3c80e3;
  text-align: center;
  vertical-align: middle;
  font-size: 40rpx;
  border-radius: 10rpx;
  background: #fff;
  margin: 40rpx auto 0;
}
.btnCenter{
  width: 193rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  text-align: center;
  vertical-align: middle;
  font-size: 36rpx;
  border-radius: 10rpx;
  background: #3c80e3;
  margin: 30rpx auto 0;
}
.alertBtn{
   width: 454rpx;
  height: 64rpx;
  line-height: 64rpx;
  background: #3c80e3;
  text-align: center;
  vertical-align: middle;
  font-size: 36rpx;
  border-radius: 10rpx;
  color: #fff;
  margin: 20rpx auto 0;
}
.alertBtn2{
   width: 454rpx;
  height: 84rpx;
  line-height: 84rpx;
  background: #3c80e3;
  text-align: center;
  vertical-align: middle;
  font-size: 36rpx;
  border-radius: 10rpx;
  color: #fff;
  margin: 20rpx auto 0;
}
