/* pages/replenishInfor/replenishInfor.wxss */
.messageTip{
  width: 93%;
  margin: 20rpx auto 20rpx;
}
.mainbox{
  width: 100%;
  background: #fff;
  height: 80rpx;
  border-bottom: 1px solid #ccc;
  overflow: hidden;
  position: relative;
}
.policeType{
  color: #000;
  width: 94%;
  margin: 20rpx auto 0;
}
.policeType radio{
  margin: 0 10rpx 0 0;
  transform:scale(0.8);  
  font-size: 38rpx;

}
.mains{
  width:38%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 35rpx;
  float: left;
  text-align: left;
  text-indent: 32rpx;
  color: #000;
}
.mainl{
  width:61%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 35rpx;
  float: left;
  text-align: center;
  overflow: hidden;
  display: flex;
  align-items: center;

}
.maininput{
  width:100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 35rpx;
  float: left;
  display: block;
  color: #000;
  text-align: left;
}
.otherText{
  width: 100%;
  background: #fff;
  height: 146rpx;
  overflow: hidden;
}
.otherTextarea{
  width: 92%;
  height: 120rpx;
  margin: 10rpx auto 0;
  border: 1px solid #ccc;
  font-size: 30rpx;
  text-indent: 20rpx;
  color: #000;
}
picker , .picker{
  width: 83%;
  height: 80rpx;
  line-height: 80rpx;
  float: left;
  color: #000;
  text-align: left;
}
.eventDate{
  width: 70%;
  height: 80rpx;
  line-height: 80rpx;
  border-right: 1px dotted #ccc;
}
.eventTime{
  width: 48%;
  height: 80rpx;
  line-height: 80rpx;
  text-indent: 20rpx;
}
.eventPlace{
  width: 89%;
}
.timeIcon{
  width: 32rpx;
  height: 32rpx;
  margin-right: 30rpx;
  background:  url('https://www.wxjigntu.top/image/timeIcon.png') no-repeat center center;
  background-size: cover; 
}
.placeIcon{
  width: 22rpx;
  height: 34rpx;
  margin-right: 30rpx;
  background:  url('https://www.wxjigntu.top/image/placeIcon.png') no-repeat center center;
  background-size: cover; 
}