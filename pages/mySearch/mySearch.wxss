/* pages/mySearch/mySearch.wxss */
/*我的申诉列表  */
.mySearchCon{
  width: 100%;
  height: 181rpx;
  background: #fff;
  color:#000;
  font-size: 32rpx;
  margin-bottom: 10rpx;
}
/*查询内容 */
.searchTitle{
  width: 90%;
  height: 70rpx;
  margin: 0 auto;
  line-height: 90rpx;
  display: flex;
   justify-content: space-between;
}
.searchName{
  width: 80%;
  height: 70rpx;
  margin: 0 auto;
  line-height: 90rpx;
  display: flex;
}
.searchStatus{
  width: 20%;
   height: 70rpx;
  margin: 0 auto;
  line-height: 90rpx;
  display: flex;
 
}
/*查询时间及结果  */
.searchTime ,  .searchResult{
  width: 90%;
  height: 50rpx;
  margin: 0 auto;
  line-height: 50rpx;
  display: flex;
  font-size: 26rpx;
  color: #a3a3a3;
}
.searchCon{
  margin-left: 15rpx;
}
.hide{
  display: none;
}
/*没有记录  */
.nolist{
  width: 100%;
  text-align: center;
  position: fixed;
  bottom: 80rpx;
}
.nolistImg{
  width: 415rpx;
  height: 555rpx;
  margin-top:200rpx;
}