/* pages/policeVideo/policeVideo.wxss */

.videoTip{
  width: 100%;
  height: 50rpx;
  background: #ffeb10;
}
.top{
  justify-content: center;
}
.videoTipText{
  font-size: 26rpx;
  color: #3c80e3;
  text-align: center;
  line-height: 50rpx;
}
.videoImg{
  width: 378rpx;
  height: 595rpx;
  margin: 59rpx auto 72rpx;
   background: url('https://www.wxjingtu.top/image/videoSample.png') no-repeat;
  background-size: 100% 100%; 
}

/*提示  */
.tipsOuter{
  width: 650rpx;
  height: 636rpx;
  background: #fff;
  border-radius: 20rpx;
  position: fixed;
  top:20%;
  left: 50%;
  margin-left: -325rpx;
}
.tipsTitle{
  text-align: center;
  margin:80rpx 0 36rpx;
  color: #3c80e3;
}
.tipsCon{
  width: 580rpx;
  font-size: 26rpx;
  margin: 0 auto;
  color: #5e5e5e;
  overflow: hidden;
}
.meslist{
  display: flex;
  margin-bottom: 10rpx;
  overflow: hidden;
}
.tipsnum{
  margin-right: 10rpx;
}
/*选中条款  */
.checkBox{
  display: flex;
  color: #9db9ee;
  font-size: 26rpx;
  bottom:20rpx;
  justify-content: center;
  align-items:center;
  margin:34rpx 0;
}
.check_icon{
  width: 40rpx;
  height: 40rpx;
  float: left;
  margin-right: 20rpx;
  margin-top: 5rpx;
  margin-bottom: 5rpx;
}
.surecheck{
     width: 25rpx;
   height: 25rpx;
   background: url('https://www.wxjingtu.top/image/tipChecked.png') no-repeat center center;
  background-size: cover; 
  /* background: red; */
}
.notcheck{
   width: 25rpx;
   height: 25rpx;
   background: url('https://www.wxjingtu.top/image/tipNoccheck.png') no-repeat center center;
   background-size: cover;  
  /* background: green; */
}
.disabled{
  background: #ccc;
}
.able{
  background: #ff3c3d;
}
.tipsBtn{
  width: 534rpx;
  height: 84rpx;
  border-radius: 42rpx;
  color: #fff;
  text-align: center;
  line-height: 84rpx;
  margin: 0 auto;
}
.tipClose{
  width: 44rpx;
  height: 44rpx;
  position: fixed;
  bottom:17%;
  left: 50%;
  margin-left: -22rpx;
}

